
{% block stylesheets %}
    <style type="text/css">
        .table {
            width: 1200px;
            max-width: 1200px;
            margin-bottom: 50px;
        }
        .styleTab {
            font-size: 12px;
        }
        table {
            border-collapse: collapse;
            background-color: transparent;
        }
        .table th, .table td {
            padding: 5px;
            vertical-align: top;
            border-top: 1px solid #eceeef;
            border-top-color: rgb(236, 238, 239);
        }
        .hourMaxCon {
            background: #bfe0f6 !important;
        }
        .dayMaxCon {
            background: #bfe0f6 !important;
        }
        .styleTab tbody .totalEnd {
            background: #e6e6e6;
        }
        .card {
            border-radius: 8px;
            margin: 60px auto 15px auto;
            background: #ffffff;
            position: relative;
            border: 0px solid rgba(0, 0, 0, 0.125);
            width: 90%;
        }
        .card .card-header {
            border-bottom: 1px solid rgba(0, 0, 0, 0.1);
        }
        .gradiente {
            background-color: #bce100 !important;
        }
        .card-header {
            padding: 7px 12px;
            margin-bottom: 0;
            background-color: #bce100;
        }
        .card .card-header .card-title {
            font-size: 18px;
            margin: 0;
            line-height: 30px;
            font-weight: 400;
            color: #000;
            vertical-align: middle;
        }
        .card-block {
            padding: 5px;
        }
    </style>
{% endblock %}

{% block body %}
{% endblock %}
